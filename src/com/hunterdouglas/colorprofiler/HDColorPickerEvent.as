package  com.hunterdouglas.colorprofiler{	import flash.events.Event;		public class HDColorPickerEvent extends Event	{				public static const FOCUS:String	= "HDColorPickerEvent_FOCUS";		public static const SELECTED:String	= "HDColorPickerEvent_SELECTED";				public var properties:Object;				public function HDColorPickerEvent(type:String, bubbles:Boolean = false, cancelable:Boolean = false, _properties:Object = null):void		{			super(type, bubbles, cancelable);			properties	= _properties;		}	}}